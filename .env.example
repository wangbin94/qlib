# Docker Compose Environment Variables
# Copy this file to .env and customize as needed

# User/Group IDs for file permissions (Linux/macOS only)
# Get your IDs with: id -u && id -g
USER_ID=1000
GROUP_ID=1000

# Qlib Configuration
QLIB_DATA_PATH=/root/.qlib/qlib_data/cn_data

# Jupyter Configuration
JUPYTER_TOKEN=
JUPYTER_PASSWORD=

# MLflow Configuration  
MLFLOW_TRACKING_URI=file:///workspace/qlib/mlruns

# Development Configuration
PYTHONPATH=/workspace/qlib